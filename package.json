{
  "name": "eximbay",
  "version": "0.1.0",
  "description": "eximbay module for Node.js",
  "main": "./dist/index.js",
  "scripts": {
    "build:copy": "cp src/client/index.html dist/client/index.html",
    "build": "npm run build:copy && babel src --out-dir dist",
    "clean": "rm -rf dist/",
    "publish": "npm run clean && npm run build:copy && npm run build && npm publish",
    "test": "mocha -R spec --require babel-register test/*.test.js",
    "watch": "npm run build:copy && babel --watch src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanziw/eximbay.git"
  },
  "keywords": [
    "eximbay"
  ],
  "author": {
    "name": "kanziw",
    "email": "kanziwoong@gmail.com",
    "url": "https://kanziw.github.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kanziw/eximbay/issues"
  },
  "homepage": "https://github.com/kanziw/eximbay#readme",
  "babel": {
    "presets": [
      "es2017"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-es2017": "6.24.1",
    "chai": "4.1.2"
  },
  "dependencies": {
    "mocha": "5.1.1",
    "partial-js": "1.1.5",
    "request": "2.85.0"
  }
}
